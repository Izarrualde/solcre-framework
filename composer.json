{
    "name": "solcre/solcre-framework-2",
    "description": "Base stuff of many internal projects",
    "type": "project",
    "license": "BSD-3-Clause",
    "keywords": [
      "solcre",
      "framework"
    ],
    "require": {
      "php": "^7.3",
      "doctrine/doctrine-orm-module": "^2.1"
    },
    "require-dev": {
      "phpunit/phpunit": "^7.1",
      "phpstan/phpstan": "^0.11.8",
      "phpstan/phpstan-doctrine": "^0.9.1",
      "squizlabs/php_codesniffer": "^3.4",
      "phpstan/phpstan-strict-rules": "^0.9.0"
    },
    "autoload": {
      "psr-4": {
        "Solcre\\SolcreFramework2\\": "./src"
      }
    },
    "autoload-dev": {
      "psr-4": {
        "SolcreFrameworkTest\\": "./tests/"
      }
    },
    "scripts": {
      "cs-check": "phpcs",
      "cs-fix": "phpcbf",
      "test": "phpunit"
    }
  }
