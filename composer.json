{
  "name": "solcre/solcre-framework-2",
  "description": "Base stuff of many internal projects",
  "type": "project",
  "license": "BSD-3-Clause",
  "keywords": [
    "solcre",
    "framework"
  ],
  "require": {
    "php": "^7.3",
    "doctrine/doctrine-orm-module": "^2.1"
  },
  "autoload": {
    "psr-4": {
      "Solcre\\SolcreFramework2\\": "./src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "SolcreFrameworkTest\\": "./tests/"
    }
  },
  "scripts": {
    "cs-check": "phpcs",
    "cs-fix": "phpcbf",
    "test": "phpunit"
  },
  "require-dev": {
    "phpunit/phpunit": "^8.4",
    "phpro/grumphp": "^0.16.2",
    "localheinz/composer-normalize": "^1.3",
    "phpstan/phpstan": "^0.11.19",
    "povils/phpmnd": "^2.1",
    "sebastian/phpcpd": "^4.1",
    "squizlabs/php_codesniffer": "^3.5",
    "roave/security-advisories": "dev-master"
  }
}
